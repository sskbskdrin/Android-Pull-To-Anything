<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullLayout">
        <attr name="pull_orientation" format="integer">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="pull_resistance_increase" format="boolean"/>
        <attr name="pull_resistance" format="float"/>
        <attr name="pull_pin_content" format="boolean"/>
        <attr name="pull_max_range" format="fraction"/>
        <attr name="pull_close_back_time" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PullLayout_Layout">
        <attr name="pull_isContentView" format="boolean"/>
        <attr name="pull_inParentPosition">
            <enum name="left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="right" value="3"/>
            <enum name="bottom" value="4"/>
        </attr>
        <attr name="android:layout_gravity"/>
    </declare-styleable>
</resources>

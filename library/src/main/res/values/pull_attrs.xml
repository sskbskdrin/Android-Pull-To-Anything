<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullLayout">
        <attr name="pull_orientation" format="integer">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="pull_content_id" format="reference"/>
        <attr name="pull_left_id" format="reference"/>
        <attr name="pull_top_id" format="reference"/>
        <attr name="pull_right_id" format="reference"/>
        <attr name="pull_bottom_id" format="reference"/>
        <attr name="pull_resistance" format="float"/>
        <attr name="pull_pin_content" format="boolean"/>
        <attr name="pull_max_range" format="fraction"/>
        <attr name="pull_close_back_time" format="integer"/>
        <attr name="pull_top" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PullLayout_Layout">
        <attr name="pull_position" format="enum">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>
</resources>